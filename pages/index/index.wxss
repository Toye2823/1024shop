.con3 .list .fr .old_price{ font-size: 24rpx;color: #b2b2b2;}
.con3 .list .fr navigator{ float: right;font-size: 30rpx;background-color: #ff4a3a;padding: 0 16rpx;color: #fff;border-radius: 20rpx;line-height: 40rpx;}
.fresh_vege image{ width: 100%;height: 230rpx;}

.con3 .list .oversell{position: absolute;left: o;top: 0;width: 250rpx;height: 250rpx;background-color: #b5b5b5;opacity: 0.8;}
