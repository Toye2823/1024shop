.quan{ 
    padding:20rpx 20rpx 0 30rpx;
    background-color: #fafafa;
    height: 100%;
    
}
.quan .list{
    background-size: 100%;
    width: 100%;
    height: 170rpx;
    margin-bottom: 20rpx;
}
.quan .uselist{ 
    background-image: url("data:image/png;base64,");
    }
.quan .nouselist{
    background-image: url("data:image/png;base64,");
}
.quan .list .fl{padding-left: 44rpx;position: relative;}
.quan .list .fl .icon{position: absolute;top: 50%;left: 0;margin-top: -14rpx; width: 44rpx;height: 170rpx;}
.quan .list .fl .icon image{ width: 28rpx; height: 28rpx;}
.quan .list .sort{ font-size: 32rpx;line-height: 60rpx;margin-top: 15rpx;}
.quan .list .date{ font-size: 24rpx;line-height: 40rpx;}
.quan .list .use{ color: #ff5050;font-size: 24rpx;}
.quan .list .nouse{ color: #333;font-size: 24rpx;}
.quan .list .fr{ width: 200rpx;text-align: right;color: #fff;padding-right: 30rpx;}
.quan .list .num{ font-size: 50rpx;height: 96rpx;line-height: 96rpx;}
.quan .list .num text{ font-size: 70rpx;}
.quan .list .text{ font-size: 22rpx;line-height: 30rpx;}

















