/* pages/login/login.wxss */
.login{
    background-image: url("data:image/png;base64,");
    width: 100%;
    height: 100%;
    background-repeat: repeat-y;
    background-size: 100%;
    padding: 0 20rpx;
}

.nav{
    font-size: 36rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
}
.list .tit{
    font-size: 30rpx;
    font-weight: bold;
    height: 60rpx;
    line-height: 60rpx;
}
.list .star{
    color: #ff4a3a;
    font-weight: blod;
}
.list .red{
    color: #ff4a3a;
    font-size: 20rpx;
    line-height: 30rpx;
    margin-bottom: 10rpx;
}
.clor{
    color: #b2b2b2;
}
.list input{
    height: 68rpx;
    line-height: 68rpx;
    background-color: #fff;
    font-size: 26rpx;
    padding: 0 20rpx;
    width: 670rpx;
    border-radius: 10rpx;
    margin-bottom: 16rpx;
}
.list button::after {
  border: none;
}
.list .tel{
    width: 710rpx;
    position: relative;
}
.list button{
    position: absolute;
    top: 0;
    right: 0;
    width: 200rpx;
    font-size: 26rpx;
    color: #fff;
    padding: 0;
    height: 68rpx;
    line-height: 68rpx;
    border-radius: 0 10rpx 10rpx 0;
}
.list button.daojishi{
    background-color: #b3b3b3;
}
.list button.getyzm{
    display: none;
    background-color: #ff4a3a;
}
.list .pro,
.list .city{
    width: 690rpx;
    position: relative;
}
.list .fr{
    margin: 40rpx 100rpx 0 0;
}
.list .fr view{
    font-size: 26rpx;
    width: 400rpx;
    line-height: 40rpx;
    text-align: center;
}
.kailong{
	width:0;
	height:0;
	border-right:20rpx solid transparent;
	border-left:20rpx solid transparent;
	border-top:20rpx solid #333;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -10rpx;
}
 radio-group{
  /* display: flex;
  align-items: center; */
  background-color: #fff;
  width: 670rpx;
  padding: 20rpx;
  border-radius: 10rpx;
}
 radio-group .radio{
  font-size: 26rpx;
  display: block;
  width: 690rpx;
  color: #b2b2b2;
}
 radio-group radio{
    font-size: 20rpx; 
  transform:scale(0.6);
}

.addImv{
    margin: 20rpx 20rpx 20rpx 0;
}
.addImv .chooseView{
    width: 160rpx;
    height: 160rpx;
    background-color: #ffffff;
    margin-left: 10rpx;
    border: 1px solid lightgray;
    border-radius: 5px;
    text-align: center;
    line-height: 180rpx;
}
.addImv .chooseImv{
    width: 160rpx;
    height: 160rpx;
}

.addImv .upFile{
    margin-left: 10rpx;
    width: 160rpx;
    height:160rpx;
    position: relative;
    border-radius: 5px;
}
.addImv .upFile .itemImv{
    width: 100%;
    height: 100%;
}
.addImv .upFile .closeImv{
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 50rpx;
    height:50rpx;
}
textarea{
    width: 670rpx;
    padding: 20rpx;
    height: 100rpx;
    font-size: 26rpx;
    line-height: 30rpx;
    background-color: #fff;
    border-radius: 10rpx;
}
.foot{
    overflow: hidden;
    width: 710rpx;
}
.foot view{
    float: left;
    width: 50%;
    padding: 0 20rpx;
    box-sizing: border-box;
    margin-top: 20rpx;
    margin-bottom: 64rpx;
}
.foot view navigator{
    background-color: #ff4a3a;
    height: 76rpx;
    line-height: 76rpx;
    color: #fff;
    text-align: center;
    font-size: 32rpx;
    border-radius: 10rpx;
}